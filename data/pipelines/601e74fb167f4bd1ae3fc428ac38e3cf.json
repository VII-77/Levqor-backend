{
  "id": "601e74fb167f4bd1ae3fc428ac38e3cf",
  "description": "When someone fills out the contact form on my website, create a task in Notion and notify me on Slack",
  "trigger": "webhook",
  "actions": [
    {
      "type": "createPage",
      "connector": "notion",
      "params": {
        "databaseId": "your_database_id",
        "title": "New Contact Form Submission",
        "content": "Details of the form submission."
      }
    },
    {
      "type": "sendSlack",
      "connector": "slack",
      "params": {
        "channel": "#notifications",
        "message": "A new contact form has been submitted."
      }
    }
  ]
}